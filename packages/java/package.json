{
  "name": "@theia/java",
  "version": "0.3.8",
  "description": "Theia - Java Extension",
  "dependencies": {
    "@theia/core": "^0.3.8",
    "@theia/editor": "^0.3.8",
    "@theia/languages": "^0.3.8",
    "@theia/monaco": "^0.3.8",
    "@types/glob": "^5.0.30",
    "glob": "^7.1.2"
  },
  "devDependencies": {
    "@theia/ext-scripts": "^0.3.8",
    "gulp": "^3.9.1",
    "gulp-decompress": "^2.0.1",
    "gulp-download": "0.0.1"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "theiaext clean",
    "build": "concurrently -n download,build -c red,blue \"gulp download_java_server\" \"theiaext build\"",
    "watch": "theiaext watch",
    "test": "theiaext test",
    "docs": "theiaext docs"
  },
  "publishConfig": {
    "access": "public"
  },
  "theiaExtensions": [
    {
      "frontend": "lib/browser/java-frontend-module",
      "backend": "lib/node/java-backend-module"
    }
  ],
  "keywords": [
    "theia-extension"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/theia-ide/theia.git"
  },
  "bugs": {
    "url": "https://github.com/theia-ide/theia/issues"
  },
  "homepage": "https://github.com/theia-ide/theia",
  "files": [
    "lib",
    "src"
  ],
  "nyc": {
    "extends": "../../configs/nyc.json"
  }
}
